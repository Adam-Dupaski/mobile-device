<?xml version="1.0" encoding="utf-8"?>
<widget id="MobileDevice.widget.MobileDevice" offlineCapable="true" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>Mobile Device</name>
    <description>Get device information</description>
    <icon></icon>
    <properties>
        <property key="deviceEntity" type="entity" required="false">
            <caption>Device entity</caption>
            <category>Data source</category>
            <description>The entity of the device information</description>
        </property>
        <property key="deviceIdAttribute" type="attribute" entityProperty="deviceEntity" required="false">
            <caption>Device ID attribute</caption>
            <category>Data source</category>
            <description>The platform unique identification of the device</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="deviceTypeAttribute" type="attribute" entityProperty="deviceEntity" required="false">
            <caption>Device type attribute</caption>
            <category>Data source</category>
            <description>The platform operating system or make of the device</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="appVersionVersionAttribute" type="attribute" entityProperty="deviceEntity" required="false">
            <caption>Version attribute</caption>
            <category>Data source</category>
            <description>Mendix version of the application</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="appVersionBuildAttribute" type="attribute" entityProperty="deviceEntity" required="false">
            <caption>Build attribute</caption>
            <category>Data source</category>
            <description>The cordova build number of the application</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="onDeviceReadyAction" type="enumeration" defaultValue="doNothing">
            <caption>On get device information</caption>
            <category>Events</category>
            <description/>
            <enumerationValues>
                <enumerationValue key="doNothing">Do nothing</enumerationValue>
                <enumerationValue key="showPage">Show a page</enumerationValue>
                <enumerationValue key="callMicroflow">Call a microflow</enumerationValue>
            </enumerationValues>
        </property>
        <property key="microflow" type="microflow" entityProperty="deviceEntity" required="false">
            <caption>Microflow</caption>
            <category>Events</category>
            <description>Microflow to execute when the device information is retrieved</description>
            <returnType type="Void" />
        </property>
        <property key="page" type="form" required="false">
            <caption>Page</caption>
            <category>Events</category>
            <description>The page to open when device information is retrieved</description>
        </property>
        <property key="onNavigateBack" type="boolean" defaultValue="false">
            <caption>Close page</caption>
            <category>Advanced</category>
            <description>Enable this setting when you want to close the current page so that the user cannot navigate back to the page</description>
        </property>
    </properties>
</widget>
